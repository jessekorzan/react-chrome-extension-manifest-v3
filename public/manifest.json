{
    "name": "Emptycan Demo",
    "version": "0.0.1",
    "manifest_version": 3,
    "description": "React Chrome Extension Manifest v3",
    "homepage_url": "http://emptycan.com",
    "icons": { 
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png" 
    },
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_icon": {
            "19": "icon19.png",
            "38": "icon38.png"
        },
        "default_title": "React Chrome Extension Manifest v3"
    },
    "permissions": ["activeTab", "scripting"]
}